{
  "entryPoints": [
    "./src"
  ],
  "out": "docs",
  "theme": "markdown",
  "plugin": ["typedoc-plugin-markdown"],
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "name": "DNS Validator",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "readme": "README.md",
  "exclude": ["**/*.test.ts", "**/*.spec.ts", "**/tests/**/*"],
  "categorizeByGroup": true,
  "categoryOrder": [
    "Types",
    "Validators",
    "Enhanced Validators",
    "DNSSEC Validators",
    "Utilities",
    "Performance",
    "Errors"
  ],
  "sort": ["source-order"],
  "gitRevision": "main",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": true,
  "logLevel": "Warn"
}
